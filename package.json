{
  "name": "ory-admin-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run start:react",
    "start:react": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "build:css": "postcss src/themes/styles/index.css -o src/themes/styles/tailwind.css",
    "watch:css": "chokidar 'src/themes/styles/*.css' 'src/components/**/*.css' --ignore src/themes/styles/tailwind.css -c 'npm run build:css'"
  },
  "eslintConfig": {
    "extends": "react-app",
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ]
        }
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.20.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.0",
    "emotion-theming": "^10.0.27",
    "mobx": "^5.15.6",
    "mobx-react-lite": "^2.1.0",
    "mobx-state-tree": "^3.17.2",
    "navi": "^0.14.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-navi": "^0.14.4",
    "react-navi-helmet-async": "^0.14.4",
    "react-scripts": "3.4.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "autoprefixer": "^9.8.6",
    "chokidar-cli": "^2.1.0",
    "postcss-import": "^12.0.1",
    "react-app-rewired": "^2.1.6"
  }
}
